# Event counter for event $(CODE)
#
# Macros:
#  $(SYS,undefined) 	= SYS
#  $(D,undefined) 		= DEVICE
#  $(CODE,undefined) 	= Event code (hardware)


record(calc, "$(SYS,undefined)-$(D,undefined):Evt$(CODE,undefined)Cnt-Sft-I") {
  field(CALC, "A+1")
  field(INPA, "$(SYS,undefined)-$(D,undefined):Evt$(CODE,undefined)Cnt-Sft-I NPP")
  field(SCAN, "Event")
  field(EVNT, "$(CODE,undefined)")
 }












